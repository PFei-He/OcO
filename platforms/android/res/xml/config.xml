<?xml version='1.0' encoding='utf-8'?>
<widget id="top.faylib.oco" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="top.faylib.plugin.device.DevicePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Network">
        <param name="android-package" value="top.faylib.plugin.network.NetworkPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Application">
        <param name="android-package" value="top.faylib.oco.plugin.ApplicationPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>OcO</name>
    <description>
        Hybrid App Demo
    </description>
    <author email="498130877@qq.com" href="http://faylib.top">
        FayLib
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
